                                                            <summary>{{ k }}:</summary>
                                                            <ul>
                                                                {% for a, b in v|items %}
                                                                    {% if a != "methods" %}
                                                                        <li>{{ a }}: {{ b }}</li>
                                                                    {% endif %}
                                                                    {% if a == "methods" %}
                                                                        <li>{{ a }}:</li>
                                                                        <ul>
                                                                            {% for i in b %}
