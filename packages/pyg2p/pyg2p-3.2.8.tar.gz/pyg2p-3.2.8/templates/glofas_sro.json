{
    "Execution": {
        "@name": "multi_sro",
        "Aggregation": {
            "@step": 24, 
            "@type": "accumulation"
        }, 
        "OutMaps": {
            "@cloneMap": "/dataset/maps/global/dem.map", 
            "@fmap": 1, 
            "@namePrefix": "psro", 
            "@unitTime": 24, 
            "Interpolation": {
                "@latMap": "/dataset/maps/global/lat.map", 
                "@lonMap": "/dataset/maps/global/lon.map", 
                "@mode": "grib_nearest"
            }
        }, 
        "Parameter": {
            "@applyConversion": "m2mm", 
            "@shortName": "sro", 
            "@tend": 360, 
            "@tstart": 0
        }
    }
}